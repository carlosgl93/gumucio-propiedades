{
  "name": "gumucio-propiedades",
  "version": "1.0.0",
  "type": "module",
  "description": "Sitio web para Gumucio Propiedades - Agente inmobiliaria en Rancagua, Chile",
  "homepage": "https://gumucio-propiedades.web.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosgl-93/gumucio-propiedades.git"
  },
  "bugs": {
    "url": "https://github.com/carlosgl-93/gumucio-propiedades/issues",
    "email": "cgumucio93@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prepare": "husky install && cp -n env/.shared .env &",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:check": "eslint --max-warnings=0 \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit",
    "test:unit": "vitest src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "deploy": "pnpm run build && firebase deploy --only hosting && curl -X PURGE https://gumucio-pro.web.app/*",
    "emulators": "firebase emulators:start ",
    "seed:db": "npx tsx ./src/utils/seed-db.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@mui/utils": "^7.1.1",
    "@tanstack/react-query": "^5.83.0",
    "@toolpad/core": "^0.12.1",
    "dotenv": "^16.6.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.4.0",
    "firebase-tools": "^14.9.0",
    "jotai": "^2.12.1",
    "jspdf": "^4.1.0",
    "openai": "^5.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-router": "^7.2.0",
    "react-share": "^5.2.2",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "happy-dom": "^20.3.1",
    "https-localhost": "^4.7.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0"
    ]
  },
  "author": {
    "name": "Carlos Gumucio",
    "email": "cgumucio93@gmail.com"
  },
  "keywords": [
    "react",
    "pwa",
    "starter-kit",
    "vite"
  ]
}
